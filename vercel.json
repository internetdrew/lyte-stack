{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [{ "source": "/trpc/:path*", "destination": "/server/index.ts" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.html" }]
}
